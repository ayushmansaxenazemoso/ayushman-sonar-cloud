{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
      "paths": {
        "@assets/*": ["assets/*"],
        "@components/*": ["components/*"]
      }
    },
    "include": ["src"]
  }
  